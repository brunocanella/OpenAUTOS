OIL_VERSION = "2.5" "DESC_OIL_VERSION";

CPU Linux {
	OS OpenAUTOS {
		STATUS = EXTENDED;
		STARTUPHOOK = TRUE;
		ERRORHOOK = TRUE;
		SHUTDOWNHOOK = FALSE;
		PRETASKHOOK = FALSE;
		POSTTASKHOOK = FALSE;
		USEGETSERVICEID = TRUE;
		USEPARAMETERACCESS = TRUE;
		USERESSCHEDULER = TRUE;
	} "DESC_OS";

	TASK task_green {
		PRIORITY = 1;
		SCHEDULE = FULL;
		ACTIVATION = 1;
		AUTOSTART = FALSE;
	};
	TASK task_yellow {
		PRIORITY = 2;
		SCHEDULE = FULL;
		ACTIVATION = 1;
		AUTOSTART = FALSE;
	};
	TASK task_red {
		PRIORITY = 3;
		SCHEDULE = FULL;
		ACTIVATION = 1;
		AUTOSTART = FALSE;
	};
	TASK task_init {
		PRIORITY = 254;
		SCHEDULE = FULL;
		ACTIVATION = 1;
		AUTOSTART = TRUE {
			APPMODE = AppMode0;
		};
	};
};
