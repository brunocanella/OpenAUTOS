OIL_VERSION = "2.5" "DESC_OIL_VERSION";

CPU Linux {
	OS OpenAUTOS {
		STATUS = EXTENDED;
		STARTUPHOOK = TRUE;
		ERRORHOOK = TRUE;
		SHUTDOWNHOOK = FALSE;
		PRETASKHOOK = FALSE;
		POSTTASKHOOK = FALSE;
		USEGETSERVICEID = TRUE;
		USEPARAMETERACCESS = TRUE;
		USERESSCHEDULER = TRUE;
	} "DESC_OS";

	TASK task1 {
		PRIORITY = 1;
		SCHEDULE = FULL;
		ACTIVATION = 1;
		AUTOSTART = TRUE {
			APPMODE = AppMode0;
		};
		RESOURCE = Resource1;
		RESOURCE = Resource2;
		RESOURCE = Resource3;
	} "DESK_TASK_1";

	TASK task2 {
		PRIORITY = 2;
		SCHEDULE = FULL;
		ACTIVATION = 3;
		AUTOSTART = FALSE;
		RESOURCE = Resource2;
	};

	TASK task3 {
		PRIORITY = 8;
		SCHEDULE = FULL;
		ACTIVATION = 1;
		AUTOSTART = FALSE;
	};

	RESOURCE Resource1 {
		RESOURCEPROPERTY = STANDARD;
	};
	RESOURCE Resource2 {
		RESOURCEPROPERTY = STANDARD;
	};
	RESOURCE Resource4 {
		RESOURCEPROPERTY = STANDARD;
	};
	RESOURCE Resource5 {
		RESOURCEPROPERTY = STANDARD;
	};

} "DESC_CPU";
